config service "oray_com"
	option enabled "0"
	option interface "wan"
	option use_syslog "0"
	option domain ""
	option username ""
	option password ""
	option ip_source "web"
	option ip_url "http://ddns.oray.com/checkip"
	option update_url "http://[USERNAME]:[PASSWORD]@ddns.oray.com/ph/update?hostname=[DOMAIN]&myip=[IP]"

config service "3322_org"
	option enabled "0"
	option interface "wan"
	option use_syslog "0"
	option service_name "3322.org"
	option domain ""
	option username ""
	option password ""
	option ip_source "web"
	option ip_url "http://ip.3322.org"
 
