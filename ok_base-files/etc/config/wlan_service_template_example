config ServiceTemplate0 ServiceTemplate0
    option service_template enabled #enabled/.*
    option cipher ccmp #none/ccmp/ccmp-tkip/tkip/wep108/wep40
    option radius_scheme "name"
    option acl "name"
    option portal_scheme "aa" #name
    option ssid_time_range "name"
    option beacon_ssid_hide disabled #enabled/disabled
    option client_max 127
    option authentication open #open/shared/wpa-auto-psk/wpa-psk/wpa2-psk/wap2-radius
    option ptk_lifttime 3600
    option gtk_lifttime 86400
    option ptk_enabled enabled #enabled/disabled
    option gtk_enabled enabled #enabled/.*
    option wep_key_slot 128
    option psk_key_type passphrase #passphrase/.*
    option psk_key_crypt plain #plain/.*
    option wep40_key_type_1 passphrase #passphrase/.*
    option wep40_key_crypt_1 " " #plain/.*
    option wep108_key_type_1 " " #passphrase/.*
    option wep108_key_crypt_1 " " #plain/.*
    option ssid "name"
    option wep40_key_1 "string"
    option wep108_key_1 "string"
    option psk_key "string"
    option dynamic_uplink_ratelimit 20
    option dynamic_downlink_ratelimit 20
    option static_uplink_ratelimit 20
    option static_downlink_ratelimit 20
    option m2u_enable enabled #enabled/.*
    option pmf optional #mandatory/optional
    option manage_template "aa" #.*
    option manage_ip 192.168.1.1
    option manage_mask 24


config ServiceTemplate1 ServiceTemplate1
    option service_template enabled #enabled/.*
    option cipher ccmp #none/ccmp/ccmp-tkip/tkip/wep108/wep40
    option radius_scheme "name"
    option acl "name"
    option portal_scheme "bb"
    option ssid_time_range "name"
    option beacon_ssid_hide disabled #enabled/disabled
    option client_max 127
    option authentication open #open/shared/wpa-auto-psk/wpa-psk/wpa2-psk/wap2-radius
    option ptk_lifttime 3600
    option gtk_lifttime 86400
    option ptk_enabled enabled #enabled/disabled
    option wep_key_slot 128
    option psk_key_type passphrase #passphrase/.*
    option psk_key_crypt plain #plain/.*
    option wep40_key_type_1 passphrase #passphrase/.*
    option wep40_key_crypt_1 " " #plain/.*
    option wep108_key_type_1 " " #passphrase/.*
    option wep108_key_crypt_1 " " #plain/.*
    option ssid "name"
    option wep40_key_1 "string"
    option wep108_key_1 "string"
    option psk_key "string"
    option dynamic_uplink_ratelimit 20
    option dynamic_downlink_ratelimit 20
    option static_uplink_ratelimit 20
    option static_downlink_ratelimit 20
    option m2u_enable enabled #enabled/.*
    option pmf optional #mandatory/optional
    option manage_template "aa" #.*
