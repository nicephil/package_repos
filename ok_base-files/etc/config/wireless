config wifi-device  wifi0
	option type	qcawifi
	option channel	auto
    option macaddr 00:11:22:33:44:55
	option hwmode	11n
	option disabled 0

config wifi-iface
    option ifname ath0
	option device	wifi0
	option network	lan
	option mode	ap
	option ssid	oakridg-def0
	option encryption none

config wifi-iface
    option ifname ath0.1
	option device	wifi0
	option network	lan
	option mode	ap
	option ssid	oakridg-xx
	option encryption none



config wifi-device  wifi1
	option type	qcawifi
	option channel	auto
    option macaddr 00:11:22:33:44:55
	option hwmode	11ac
	option disabled 0

config wifi-iface
    option ifname ath1
	option device	wifi1
	option network	lan
	option mode	ap
	option ssid	oakridg-def1
	option encryption none

config wifi-iface
    option ifname ath1.1
	option device	wifi1
	option network	lan
	option mode	ap
	option ssid	oakridg-yy
	option encryption none


